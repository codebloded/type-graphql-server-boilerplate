{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["google", "prettier"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["error"],
        "new-cap": "off",
        "require-jsdoc": "off"
    }
}
